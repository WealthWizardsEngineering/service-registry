{
  "name": "service-registry",
  "version": "0.0.6",
  "engines": {
    "node": "6.2.0"
  },
  "description": "A RESTful service for managing service information",
  "main": "main.js",
  "scripts": {
    "start": "node main.js",
    "test:unit": "tape 'test/unit/**/*.spec.js'",
    "test:unit:watch": "nodemon -q -x 'npm run test:unit | tap-spec --silent'",
    "test:unit:coverage": "nyc --report-dir=coverage/unit --reporter=text --reporter=html --reporter=lcov npm run test:unit",
    "test:unit:report": "npm run test:unit:coverage | tee /dev/stderr | node_modules/tap-xunit/bin/tap-xunit > unit-test-report.xml",
    "test:component": "tape 'test/component/**/*.spec.js'",
    "test:component:coverage": "nyc --report-dir=coverage/component --reporter=text --reporter=html --reporter=lcov npm run test:component",
    "test:component:report": "npm run test:component:coverage | tee /dev/stderr | node_modules/tap-xunit/bin/tap-xunit > component-test-report.xml",
    "lint": "eslint *.js",
    "lint:fix": "eslint --fix *.js"
  },
  "author": "Wealth Wizards",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "conventional-recommended-bump": "1.0.1",
    "eslint": "4.6.1",
    "eslint-config-airbnb-base": "12.0.0",
    "eslint-plugin-import": "2.7.0",
    "istanbul": "0.4.5",
    "nock": "9.0.14",
    "nodemon": "1.12.0",
    "nsp": "2.7.0",
    "nyc": "11.1.0",
    "proxyquire": "1.8.0",
    "sinon": "3.2.1",
    "sinon-as-promised": "4.0.3",
    "supertest": "3.0.0",
    "tap-spec": "4.1.1",
    "tap-xunit": "1.7.0",
    "tape": "4.8.0",
    "timekeeper": "2.0.0",
    "ww-jenkins-pipeline-utils": "^1.1.1"
  },
  "dependencies": {
    "async": "^2.1.4",
    "body-parser": "1.18.2",
    "co": "4.6.0",
    "cors": "2.8.4",
    "dotenv": "4.0.0",
    "envalid": "4.0.1",
    "express": "4.16.2",
    "extend": "3.0.1",
    "gridfs-stream": "1.1.1",
    "helmet": "3.11.0",
    "joi": "10.6.0",
    "lodash": "4.17.4",
    "mongoose": "4.13.8",
    "mpath": "0.3.0",
    "request": "2.83.0",
    "request-promise-native": "1.0.4",
    "response-time": "2.3.2",
    "underscore": "1.8.3",
    "ww-events-common": "0.0.8",
    "ww-logging": "0.4.6",
    "ww-monitoring": "0.13.9",
    "ww-swagger-ui": "2.1.10",
    "ww-utils": "0.11.18"
  }
}
